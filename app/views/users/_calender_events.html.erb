<div class="card">
  <%= month_calendar(attribute: :goods_ready_date, events: inquiries) do |date, inquiry_events| %>
    <%= date %>
    <% inquiry_events.each do |inquiry| %>
    <div>
      <%= inquiry.origin_country %> to <%= inquiry.destination_country %>
    </div>
    <% end %>
  <% end %>
</div>
